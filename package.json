    "@webcomponents/webcomponentsjs": "^2.0.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "polymer-cli": "^1.7.7",
    "standard-version": "^4.2.0",
    "validate-commit-msg": "^2.14.0",
    "wct-browser-legacy": "^1.0.1"
  },
  "resolutions": {
    "inherits": "2.0.3",
